function min(a, b) {
  return a < b ? a : b;
}

function pow(x, n) {
    return x ** n;
}

function pow(x, n) {
    let result = 1;
    for (let i = 0; i < n; i++) {
      result *= x;
    }
    return result;
}

let a = 1, b = 1;

let c = ++a; // a becomes 2, and c is assigned 2
let d = b++; // b is assigned to d (which is 1), then b becomes 2

function reverseString(str) {
    return str.split('').reverse().join('');
  }
  
  console.log(reverseString("As the Zapatistas might say, let us approach the internet at the speed of dreams."));

  function converter(celsius) {
    return (celsius * 1.8) + 32;
  }
  
  console.log(converter(0)); 
  
  